@using System.Security.Cryptography
@using ASPNZBat.ViewModels
@using Humanizer
@using Microsoft.CodeAnalysis.CSharp
@model ASPNZBat.Models.SeatBookingVM
@{
    ViewData["Title"] = "_Stats";
}



<div style="overflow-x:auto;">
    <div class="container-fluid ui-btn ui-corner-all col-md-offset-2 trigger">
        <p>
            Full Session = <span id="full" style="color: Red">@ViewData["maxseats"]</span>
            Nearly Full = <span id="nearlyfull" style="color: Yellow">@ViewData["nearlyFullSeats"]</span>
            Plenty of seats = <span id="empty" style="color: Green">@ViewData["plentySeats"]</span>
        </p>




        <table style="width: 100%" class="table table-striped table-borderless" Id="tablestats">
            <thead>
                <tr>
                    <th>Date</th>
                    <th id="1th">S1</th>
                    <th id="2th">S2</th>
                    <th id="3th">S3</th>
                    <th id="4th">S4</th>
                    <th id="5th">S5</th>
                    <th id="6th">S6</th>
                    <th id="7th">S7</th>
                    <th id="8th">S8</th>
                    <th id="9th">S9</th>
                    <th id="10th">S10</th>
                    <th id="11th">S11</th>
                    <th id="12th">S12</th>
                    <th id="13th">S13</th>
                    <th id="14th">S14</th>
                    <th id="15th">S15</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in ViewData["CountSessions"] as List<SummaryStatsVM>)
                {
                    <tr class="itemRow">
                        <td class="date">@item.SeatDate.Date.ToShortDateString()</td>
                        <td class="sess" id="sS1">@item.S1</td>
                        <td class="sess" id="sS2">@item.S2</td>
                        <td class="sess" id="sS3">@item.S3</td>
                        <td class="sess" id="sS4">@item.S4</td>
                        <td class="sess" id="sS5">@item.S5</td>
                        <td class="sess" id="sS6">@item.S6</td>
                        <td class="sess" id="sS7">@item.S7</td>
                        <td class="sess" id="sS8">@item.S8</td>
                        <td class="sess" id="sS9">@item.S9</td>
                        <td class="sess" id="sS10">@item.S10</td>
                        <td class="sess" id="sS11">@item.S11</td>
                        <td class="sess" id="sS12">@item.S12</td>
                        <td class="sess" id="sS13">@item.S13</td>
                        <td class="sess" id="sS14">@item.S14</td>
                        <td class="sess" id="sS15">@item.S15</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

@section Scripts {
    @{

    }
}